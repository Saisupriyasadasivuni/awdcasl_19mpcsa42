import java.util.*;
class Minimum
{
public static void main(String[] args) 
{
int n,i;
Scanner sc=new Scanner(System.in);
System.out.println("enter n:"); 
n=sc.nextInt();
int a[]=new int[n];
System.out.println("enter "+n+" elements:"); 
for(i=0;i<n;i++)
{
a[i]=sc.nextInt();
}
int min=a[0];
for(i=1;i<n;i++)
{
if(a[i]<min)
min=a[i];
}
System.out.println(min); 
}
}
/*
output:
enter n:
5
enter 5 elements:
20 29 134 45 66
20
*/
