import java.util.*;
class minmax
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
int a[]=new int[n];
for(int i=0;i<n;i++)
a[i]=sc.nextInt();
Arrays.sort(a);
System.out.println("min:"+a[0]+" "+"max:"+a[n-1]);
}}

/*
Output:
6
3 2 1 56 10000 167
min:1 max:10000
*/
