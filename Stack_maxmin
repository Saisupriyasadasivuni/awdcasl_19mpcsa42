import java.util.*;
class stack_maxmin
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
System.out.print("enter size and elements:");
int n=sc.nextInt();
Stack s = new Stack();
for(int i=0;i<n;i++)
s.push(sc.nextInt());
System.out.println("max = "+Collections.max(s));
System.out.println("min = "+Collections.min(s));
}
}
